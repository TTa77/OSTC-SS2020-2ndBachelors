打印以下图形
    *
   ***
  *****
 *******
*********

//C
include<stdio.h>
int main(){
printf("    *");
printf("   ***");
printf("  *****");
printf(" *******");
printf("*********");
return 0;
}

# Python3
print('''    *
   ***
  *****
 *******
*********''')

// JAVA
public class Triangle{
	public static void main(String arg[]){
		System.out.println("    *");
		System.out.println("   ***");
		System.out.println("  *****");
		System.out.println(" *******");
		System.out.println("*********");

// C++
#include <iostream>
int main()
{
	std::cout << "    *    " << std::endl;
	std::cout << "   ***   " << std::endl;
	std::cout << "  *****  " << std::endl;
	std::cout << " ******* " << std::endl;
	std::cout << "*********" << std::endl;
	return 0;
 } 

//Python
row = int(input('请输入行数: '))
for i in range(row):
    for _ in range(i + 1):
        print('*', end='')
    print()
