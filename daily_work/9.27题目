题目：一个整数，它加上100后是一个完全平方数，再加上268又是一个完全平方数，请问该数是多少？
	

//JAVA Yutong Guo
public class Find_num {
	public static void main(String arg[]) {
		boolean status = true;
		int num = 1;
		while (status) {
			double a = Math.pow(num+100,0.5)%1;
			double b = Math.pow(num+368,0.5)%1;
			if ((a == 0) && (b == 0)){
				System.out.print(num);
				status = false;
			}else {
				num ++;
			}
		}
	}

}

# Python3 Wen Mingyuan
for first_num in range(-100, 40000):
    second_num = first_num + 100  
    third_num = second_num + 268
    for i in range(0, 201):
        if second_num == i * i:
            for j in range(0, 201):
                if third_num == j * j:
                    print(first_num)
                    break

// C Zhang Tianning
#include "math.h" 
main()
{
   long int i, x, y, z;
   for (i = 1; i < 100000; i++)
   {
      x = sqrt(i + 100); 
      y = sqrt(i + 268); 
      if (x*x == i + 100 && y*y == i + 268)
         printf("\n%ld\n", i);
   }
}





// python3 Chen Ruoyu
from math import sqrt
for i in range(-100, 10000):
    m = int(sqrt(i + 100))
    n = int(sqrt(i + 268))
    if m * m == i + 100 and n * n == i +268:
        print(i)
