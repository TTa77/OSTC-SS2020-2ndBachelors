题目：一个5位数，判断它是不是回文数。即12321是回文数，个位与万位相同，十位与千位相同。

//C ZhangTianning
#include <stdio.h>
 
int main( )
{
    long ge,shi,qian,wan;
    long x;
    printf("请输入 5 位数字：");
    scanf("%ld",&x);
    wan=x/10000;
    qian=x%10000/1000;
    shi=x%100/10;
    ge=x%10;
    if (ge==wan&&shi==qian) {
        printf("这是回文数\n");
    } else {
        printf("这不是回文数\n");
    }
}

//JAVA YutongGuo

import java.util.Scanner;
public class Palindromic {
    public static void main(String arg[]) {
        Scanner input = new Scanner(System.in);
        System.out.print("请输入一个五位数: ");
        int num = input.nextInt();
        int[] a = new int[5];

        for (int i = 4; i >=  0; i--) {
            a[i] = num % 10;
            num /= 10;
        }

        if ((a[0] == a[4]) && (a[1] == a[3])){
            System.out.println("这是回文数");
        }else {
            System.out.println("这不是回文数");
        }
    }

}

def huiwenshu(a):
    b = a
    c = 0
    while b > 0:
        c *= 10
        c += b % 10
        b //= 10
    return c == a
print(huiwenshu())

# Python3 Wen Mingyuan
while True:
    num = input('请输入一个5位数：')
    if len(num) != 5:
        print('输入位数有误！请重新输入。')
        continue
    if num[0] == num[-1] and num[1] == num[-2]:
        print(num + '是回文数')
    else:
        print(num + '不是回文数')

// C++ Luo Haoming
#include <iostream>
int main()
{
	std::cout << "请输入一个五位数:" << std::endl;
	int number = 0, number1 = 0, number2 = 0, number3 = 0, number4 = 0, number5 = 0;
	std::cin >> number;
	number1 = number / 10000;
	number2 = (number - 10000 * number1) / 1000;
	number3 = (number - 10000 * number1 - 1000 * number2) / 100;
	number4 = (number - 10000 * number1 - 1000 * number2 - 100 * number3) / 10;
	number5 = number - 10000 * number1 - 1000 * number2 - 100 * number3 - 10 * number4;
	if (number1 == number5 && number2 == number4) {
		std::cout << "这是回文数。" << std::endl;
		}
	else{
		std::cout << "这不是回文数。" << std::endl;	
		}
	return 0;
}
